{
  "name": "onedrive-api",
  "version": "0.1.3",
  "description": "OneDrive module for communicating with oneDrive API. Built using functional programming pattern",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run test:unit",
    "test:e2e": "NODE_ENV=test mocha --opts ./test/e2e/mocha.opts ./test/e2e/bootstrap.test.js ./test/e2e/**/*.test.js",
    "test:unit": "NODE_ENV=test mocha ./test/unit/*.test.js",
    "release:tag": "git tag $npm_package_version && git push --tags"
  },
  "keywords": [
    "onedrive",
    "api",
    "rest",
    "skydrive"
  ],
  "author": "Domagoj Katavic",
  "repository": {
    "type": "git",
    "url": "https://github.com/dkatavic/onedrive-api.git"
  },
  "license": "MIT",
  "dependencies": {
    "is-empty-object": "^1.1.1",
    "request": "^2.70.0",
    "request-promise": "^2.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "faker": "^3.1.0",
    "install": "0.6.1",
    "mocha": "^2.5.3",
    "npm": "3.8.6",
    "string-stream": "0.0.7"
  }
}